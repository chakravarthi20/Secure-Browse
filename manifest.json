{
    "manifest_version" : 3,
    "name" : "Secure Browse",
    "version" : "1.0",
    "description" : "A secure web browser",
    "background":{
        "service_worker": "background/background.js"
    },
    "permissions": [
        "downloads",
        "webRequest",
        "webNavigation",
        "notifications",
        "tabs",
        "scripting",
        "activeTab"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["common/content_script.js"],
          "run_at": "document_end"
        }
    ],
    "host_permissions": 
        ["<all_urls>"],
    "action":{
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "asset/icon.png",
            "48": "asset/icon.png",
            "128": "asset/icon.png"
        }
    }

}